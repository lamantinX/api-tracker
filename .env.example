# PostgreSQL
POSTGRES_USER=api_watcher
POSTGRES_PASSWORD=api_watcher_secret
POSTGRES_DB=api_watcher

# Логирование
LOG_LEVEL=INFO

# AI анализ (опционально)
OPENROUTER_API_KEY=
OPENROUTER_MODEL=google/gemini-2.5-flash

# Slack (опционально)
SLACK_BOT_TOKEN=
SLACK_CHANNEL=

# Webhook (опционально)
WEBHOOK_URL=

# Telegram (опционально)
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=

# ZenRows для обхода защиты (опционально)
ZENROWS_API_KEY=

# Предохранители (рекомендуется при включённом ZenRows)
# -1 = безлимит, 0 = запретить ZenRows, >0 = максимум запросов/день
API_WATCHER_ZENROWS_DAILY_REQUEST_LIMIT=2000
# direct_first = сначала прямой запрос, ZenRows только при ошибке/блокировке (рекомендуется)
# zenrows_first = всегда через ZenRows (дорого)
API_WATCHER_ZENROWS_STRATEGY=direct_first
# Не использовать ZenRows для .json/.yaml/.yml/raw.githubusercontent.com
API_WATCHER_ZENROWS_SKIP_STATIC=true
# Настройки ZenRows (могут влиять на стоимость)
API_WATCHER_ZENROWS_ANTIBOT=false
API_WATCHER_ZENROWS_JS_RENDER=true
# Минимальный интервал в daemon режиме (сек). Если CHECK_INTERVAL меньше — будет поднят до этого значения.
API_WATCHER_MIN_CHECK_INTERVAL=300
# Разрешить частый polling (ОПАСНО при ZenRows)
API_WATCHER_ALLOW_FAST_POLL=false

# SerpAPI для поиска документации (опционально)
SERPAPI_KEY=
